{
    "name": "distrimedia/magento2-distrimedia-connector",
    "description": "DistriMedia Connector module",
    "type": "magento2-module",
    "keywords": ["magento", "magento2", "distrimedia"],
    "authors": [
        {
            "name": "Tristan Hofman",
            "email": "tristan@baldwin.be"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "~7.1.0 || ~7.2.0 || ~7.3.0 || ~7.4.0",
        "magento/framework": "^102.0.0",
        "distrimedia/soap-client": "dev-master",
        "magento/module-sales": "^102.0.5",
        "magento/module-catalog-inventory": "^100.3.5",
        "magento/module-asynchronous-operations": "^100.3.5"
    },
    "require-dev": {
        "bitexpert/phpstan-magento": "^0.3.0",
        "ergebnis/composer-normalize": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpcompatibility/php-compatibility": "^9.2",
        "magento/magento-coding-standard": "^5.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.26",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "DistriMedia\\Connector\\": ""
        }
    }
}
